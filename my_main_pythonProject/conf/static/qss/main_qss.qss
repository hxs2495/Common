/* QLCDNumber样式 */
QLCDNumber {
    background-color: #FFFFFF; /* 白色背景 */
    border: 1px solid #11A428; /* 绿色边框 */
    border-radius: 5px;
    padding: 5px;
    color: #11A428; /* 绿色文字 */
}

/* 设置选项卡背景色 */
QTabBar::tab {
    background-color: #FFFFFF;
}

/* 设置选中选项卡的背景色和文本颜色 */
QTabBar::tab:selected {
    background-color: #11A428;
    color: #FFFFFF;
}
/* 设置鼠标悬停时的样式 */
QTabBar::tab:hover {
    background-color: #11A428;
    color: #000000;
}

QDateTimeEdit{
    background-color: #FFFFFF; /* 白色背景 */
    border: 1px solid #11A428; /* 绿色边框 */
    border-radius: 5px;
    padding: 5px;
    color: #11A428; /* 绿色文字 */
}